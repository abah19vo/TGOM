
<title>Feedbacks</title>
	{{#if dbErrorOccurred}}
		<p class="admin_top">Can't display the page now, try again later.</p>
	{{else}}
		<p class="creat"><a href="/comments/create">Post A Feedback</a></p>
		<p class="staff_top">FEEDBACKS:</p>
		{{#if feedbacks.length}}
			<div class="displayed_list">
				{{#each feedbacks}}
					<p><a href="/feedback/{{id}}" class="id_name">{{title}}</a></p>
				{{/each}}
			</div>
		{{else}}
			<p class="no-content">Theres no Comments</p>
		{{/if}}
		
	{{/if}}
