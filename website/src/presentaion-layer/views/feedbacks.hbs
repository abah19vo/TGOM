
<title>Feedbacks</title>
{{#if dbErrorOccurred}}
    <p class="admin_top">Can't display the page now, try again later.</p>
{{else}}
    {{#if isLoggedIn}}
        <p class="creat"><a href="/feedbacks/create/{{user.id}}">Post A Feedback</a></p>
    {{/if}}
    <p class="feedbacks_top">FEEDBACKS:</p>
    {{#if feedbacks.length}}
        <div class="displayed_list">
            {{#each feedbacks}}
                <p><a href="/feedback/{{feedbacks.id}}" class="id_name">{{feedbacks.title}}</a></p>
            {{/each}}
        </div>
    {{else}}
        <p class="no-content">Theres no Comments</p>
    {{/if}}
{{/if}}
